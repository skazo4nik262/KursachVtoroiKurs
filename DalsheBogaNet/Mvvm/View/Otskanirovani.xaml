<Window x:Class="DalsheBogaNet.Mvvm.View.Otskanirovani"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DalsheBogaNet.Mvvm.View"
        xmlns:vm="clr-namespace:DalsheBogaNet.Mvvm.Model"
        mc:Ignorable="d"
        DataContext="{DynamicResource code}"
        Title="Otskanirovani" Height="450" Width="800">
    <Window.Resources>
        <vm:Code x:Key="code"/>
    </Window.Resources>
    <Viewbox VerticalAlignment="Top">
            <ListView ItemsSource="{Binding Code}">
                <ListView.View>
                    <GridView>
                        <GridViewColumn Header="ID" DisplayMemberBinding="{Binding Codes}" />
                    </GridView>
                </ListView.View>
            </ListView>
    </Viewbox>
</Window>
